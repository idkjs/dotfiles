{
	"name": "vscode-open-iterm2",
	"displayName": "Open iTerm2",
	"description": "Open iTerm2 for vscode",
	"version": "0.0.4",
	"publisher": "mushan",
	"author": {
		"name": "mushan",
		"email": "mushanmail@126.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mushanshitiancai/vscode-open-iterm2.git"
	},
	"homepage": "https://github.com/mushanshitiancai/vscode-open-iterm2/blob/master/README.md",
	"bugs": {
		"url": "https://github.com/mushanshitiancai/vscode-open-iterm2/issues"
	},
	"icon": "res/icon.png",
	"keywords": [
		"iTerm2",
		"iterm2",
		"terminal"
	],
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.openITerm2"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.openITerm2",
				"title": "Open iTerm2 Here"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.openITerm2"
				}
			],
			"editor/context": [
				{
					"command": "extension.openITerm2"
				}
			],
			"editor/title": [
				{
					"command": "extension.openITerm2"
				}
			],
			"editor/title/context": [
				{
					"command": "extension.openITerm2"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"mocha": "^2.3.3",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	},
	"__metadata": {
		"id": "027fdf55-575b-407a-ade6-f32a8923c23c",
		"publisherId": "f66fd94c-5d57-438e-9d46-cd35dbfdd5c2",
		"publisherDisplayName": "mushan"
	}
}