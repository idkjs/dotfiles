{
	"name": "duckduckgo",
	"displayName": "DuckDuckGo Search",
	"description": "Enables DuckDuckGo Search from the VS Code editor.",
	"version": "0.0.3",
	"icon": "images/logo.svg",
	"publisher": "tailot",
	"author": {
		"name": "Vincenzo Tilotta",
		"email": "tailot@gmail.com"
	},
	"license": "MIT",
	"keywords": [
		"search",
		"duckduckgo",
		"privacy",
		"bang"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/tailot/vcode-duckduckgo.git"
	},
	"bugs": {
		"url": "https://github.com/tailot/vcode-duckduckgo/issues"
	},
	"engines": {
		"vscode": "^1.12.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:duckduckgo.webSearch",
		"onCommand:duckduckgo.imagesSearch",
		"onCommand:duckduckgo.videosSearch",
		"onCommand:duckduckgo.bangSearch"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "duckduckgo.webSearch",
				"title": "Duckduckgo: Web"
			},
			{
				"command": "duckduckgo.imagesSearch",
				"title": "Duckduckgo: Images"
			},
			{
				"command": "duckduckgo.videosSearch",
				"title": "Duckduckgo: Videos"
			},
			{
				"command": "duckduckgo.bangSearch",
				"title": "Duckduckgo: !bang"
			}
		],
		"menus": {
			"editor/title": [
				{
					"when": "editorHasSelection",
					"command": "duckduckgo.webSearch",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"mocha": "^2.3.3",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	},
	"dependencies": {
		"openurl": "^1.1.1"
	},
	"__metadata": {
		"id": "98cf6c26-dad2-430d-82b1-9d290c7e93e0",
		"publisherId": "eac040c2-a688-417f-8080-9ee89022f534",
		"publisherDisplayName": "Vincenzo Tilotta"
	}
}